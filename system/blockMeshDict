FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
scale 1;

Ls 0.04013;   // length  (x)
Ws 1;   // width   (z)		// set to 1 in 2D , 0.04013 in 3D 
Hs 0.00353;   // height  (y)
pl 4;
hl 3;
yl 2;

xsb #calc "$Ls*0.5";
xmin #calc "$Ls* $pl * 0.5";
ycd #calc "$Hs* $hl";
ymin #calc "$Hs * $yl";


Nxsb 80; 
Nx 40;
Ny 20;
Nz 1;
No 80;


vertices
(
    ( -$xsb  0  0       )  // 0
    ( $xsb  0  0       )  // 1
    ( $xsb  $Hs  0       )  // 2
    ( -$xsb  $Hs  0       )  // 3

    ( -$xmin  0  0     )  // 4
    ( $xmin  0  0     )  // 5
    ( $xmin  $ycd  0     )  // 6
    ( -$xmin  $ycd  0     )  // 7

    ( -$xsb  0  $Ws       )  // 8
    ( $xsb  0  $Ws       )  // 9
    ( $xsb  $Hs  $Ws       )  // 10
    ( -$xsb  $Hs  $Ws      )  // 11

    ( -$xmin  0  $Ws     )  // 12
    ( $xmin  0  $Ws     )  // 13
    ( $xmin  $ycd  $Ws     )  // 14
    ( -$xmin  $ycd  $Ws     )  // 15

    ( -$xsb  -$Hs  0       )  // 16
    ( $xsb  -$Hs  0       )  // 17
    ( $xmin  -$Hs  0       )  // 18
    ( $xmin -$ymin  0       )  // 19
    ( $xsb  -$ymin     0   )  // 20
    ( -$xsb  -$ymin  0       )  // 21
    ( -$xmin -$ymin  0       )  // 22
    ( -$xmin  -$Hs     0   )  // 23

    ( -$xsb  -$Hs  $Ws       )  // 24
    ( $xsb  -$Hs  $Ws       )  // 25
    ( $xmin  -$Hs  $Ws       )  // 26
    ( $xmin -$ymin  $Ws       )  // 27
    ( $xsb  -$ymin  $Ws   )  // 28
    ( -$xsb  -$ymin  $Ws       )  // 29
    ( -$xmin -$ymin  $Ws       )  // 30
    ( -$xmin  -$Hs     $Ws   )  // 31


);

blocks
(
    hex (0 1 2 3 8 9 10 11) seebeckcell ($Nxsb $Ny $Nz) simpleGrading (1 1 1) 		//A
    hex (4 0 3 7 12 8 11 15) insulator ($No $Ny $Nz) simpleGrading (1 1 1)		//B	
    hex (3 2 6 7 11 10 14 15) conductor ($Nxsb $No $Nz) simpleGrading (1 1 1)		//C
    hex (1 5 6 2 9 13 14 10) insulator ($No $Ny $Nz) simpleGrading (1 1 1)		//D
    hex (17 18 5 1 25 26 13 9) insulator ($No $Ny $Nz) simpleGrading (1 1 1)		//E
    hex (20 19 18 17 28 27 26 25) insulator ($No $Ny $Nz) simpleGrading (1 1 1)		//F
    hex (21 20 17 16 29 28 25 24) insulator ($Nxsb $Ny $Nz) simpleGrading (1 1 1)		//G
    hex (22 21 16 23 30 29 24 31) insulator ($No $Ny $Nz) simpleGrading (1 1 1)		//H
    hex (23 16 0 4 31 24 8 12) insulator ($No $Ny $Nz) simpleGrading (1 1 1)		//I
);

edges ();

boundary
(
    TOP
    {
        type wall;
        faces (
		(6 7 15 14)
	      );     
    }

    WATER
    {
        type wall;
        faces (
		(0 1 9 8)
		(1 17 25 9)
		(16 17 25 24)
		(0 16 24 8)
	      );     
    }

    AIR
    {
	type wall;
	faces (
		(5 6 14 13)
		(4 7 15 12)
		(5 18 26 13)
		(18 19 27 26)
		(20 19 27 28)
		(21 20 28 29)
		(22 21 29 30)
		(22 23 31 30)
		(23 4 12 31)
	     );
    }


    FRONT_BACK
    {
        type empty;
        faces (
		(0 1 2 3)
              	(8 9 10 11)
		(1 5 6 2)
		(9 13 14 10)
		(3 2 6 7)
		(11 10 14 15)
		(4 0 3 7)
		(12 8 11 15)
		(17 18 5 1)
		(25 26 13 9)
		(20 19 18 17)
		(28 27 26 25)
		(21 20 17 16)
		(29 28 25 24)
		(22 21 16 23)
		(30 29 24 31)
		(23 16 0 4)
		(31 24 8 12)
		);
    }
);

mergePatchPairs ();

