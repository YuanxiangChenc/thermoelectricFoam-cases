FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

scale 1;

//------------------------------- Geometry -------------------------------
Ls 0.04013;   // seebeckcell length in x
Ws 0.04013;   // depth in z
Hs 0.00353;   // half-layer height reference in y
xd 0.001;     // fin thickness (x)

pl 4;
hl 3;
yl 2;

xsb  #calc "$Ls*0.5";          // x in [-xsb, +xsb]
xmin #calc "$Ls*$pl*0.5";      // outer top half-length
ycd  #calc "$Hs*$hl";          // top y
ymin #calc "$Hs*$yl";          // bottom magnitude (y=-ymin)

//------------------------------- Fin centres (7 fins) -------------------
cf3l #calc "-$xsb*3/4";
cf2l #calc "-$xsb*2/4";
cf1l #calc "-$xsb*1/4";
cf0  #calc "0";
cf1r #calc  "$xsb*1/4";
cf2r #calc  "$xsb*2/4";
cf3r #calc  "$xsb*3/4";

// fin edges
lf3l #calc "$cf3l - 0.5*$xd";  rf3l #calc "$cf3l + 0.5*$xd";
lf2l #calc "$cf2l - 0.5*$xd";  rf2l #calc "$cf2l + 0.5*$xd";
lf1l #calc "$cf1l - 0.5*$xd";  rf1l #calc "$cf1l + 0.5*$xd";
lf0  #calc "$cf0  - 0.5*$xd";  rf0  #calc "$cf0  + 0.5*$xd";
lf1r #calc "$cf1r - 0.5*$xd";  rf1r #calc "$cf1r + 0.5*$xd";
lf2r #calc "$cf2r - 0.5*$xd";  rf2r #calc "$cf2r + 0.5*$xd";
lf3r #calc "$cf3r - 0.5*$xd";  rf3r #calc "$cf3r + 0.5*$xd";

// top-mapped x (at y=ycd): precompute (NO inline arithmetic in vertices)
lf3l_t #calc "$lf3l*$pl";  rf3l_t #calc "$rf3l*$pl";
lf2l_t #calc "$lf2l*$pl";  rf2l_t #calc "$rf2l*$pl";
lf1l_t #calc "$lf1l*$pl";  rf1l_t #calc "$rf1l*$pl";
lf0_t  #calc "$lf0 *$pl";  rf0_t  #calc "$rf0 *$pl";
lf1r_t #calc "$lf1r*$pl";  rf1r_t #calc "$rf1r*$pl";
lf2r_t #calc "$lf2r*$pl";  rf2r_t #calc "$rf2r*$pl";
lf3r_t #calc "$lf3r*$pl";  rf3r_t #calc "$rf3r*$pl";

//------------------------------- Mesh resolution ------------------------
Nxsb 78;    // total in seebeckcell x
Ny   20;
Nz   1;
No   80;    // y-cells for outer blocks / top region
Nf   2;     // x-cells across each fin thickness

// Gap x-cells (8 gaps around 7 fins): must sum to Nxsb - 7*Nf
Ng0 8;
Ng1 8;
Ng2 8;
Ng3 8;
Ng4 8;
Ng5 8;
Ng6 8;
Ng7 8;

//------------------------------- Vertices --------------------------------
vertices
(
    // Base (original) corners
    ( -$xsb  0   0   )  // 0
    (  $xsb  0   0   )  // 1
    (  $xsb  $Hs 0   )  // 2
    ( -$xsb  $Hs 0   )  // 3

    ( -$xmin 0   0   )  // 4
    (  $xmin 0   0   )  // 5
    (  $xmin $ycd 0  )  // 6
    ( -$xmin $ycd 0  )  // 7

    ( -$xsb  0   $Ws )  // 8
    (  $xsb  0   $Ws )  // 9
    (  $xsb  $Hs $Ws )  // 10
    ( -$xsb  $Hs $Ws )  // 11

    ( -$xmin 0   $Ws )  // 12
    (  $xmin 0   $Ws )  // 13
    (  $xmin $ycd $Ws ) // 14
    ( -$xmin $ycd $Ws ) // 15

    ( -$xsb  -$Hs   0   )  // 16
    (  $xsb  -$Hs   0   )  // 17
    (  $xmin -$Hs   0   )  // 18
    (  $xmin -$ymin 0   )  // 19
    (  $xsb  -$ymin 0   )  // 20
    ( -$xsb  -$ymin 0   )  // 21
    ( -$xmin -$ymin 0   )  // 22
    ( -$xmin -$Hs   0   )  // 23

    ( -$xsb  -$Hs   $Ws )  // 24
    (  $xsb  -$Hs   $Ws )  // 25
    (  $xmin -$Hs   $Ws )  // 26
    (  $xmin -$ymin $Ws )  // 27
    (  $xsb  -$ymin $Ws )  // 28
    ( -$xsb  -$ymin $Ws )  // 29
    ( -$xmin -$ymin $Ws )  // 30
    ( -$xmin -$Hs   $Ws )  // 31

    // --- x = lf3l plane (y=0, Hs, -Hs, -ymin, ycd) ---
    ( $lf3l    0      0   )  // 32
    ( $lf3l    0      $Ws )  // 33
    ( $lf3l    $Hs    0   )  // 34
    ( $lf3l    $Hs    $Ws )  // 35
    ( $lf3l   -$Hs    0   )  // 36
    ( $lf3l   -$Hs    $Ws )  // 37
    ( $lf3l   -$ymin  0   )  // 38
    ( $lf3l   -$ymin  $Ws )  // 39
    ( $lf3l_t  $ycd   0   )  // 40
    ( $lf3l_t  $ycd   $Ws )  // 41

    // --- x = rf3l ---
    ( $rf3l    0      0   )  // 42
    ( $rf3l    0      $Ws )  // 43
    ( $rf3l    $Hs    0   )  // 44
    ( $rf3l    $Hs    $Ws )  // 45
    ( $rf3l   -$Hs    0   )  // 46
    ( $rf3l   -$Hs    $Ws )  // 47
    ( $rf3l   -$ymin  0   )  // 48
    ( $rf3l   -$ymin  $Ws )  // 49
    ( $rf3l_t  $ycd   0   )  // 50
    ( $rf3l_t  $ycd   $Ws )  // 51

    // --- x = lf2l ---
    ( $lf2l    0      0   )  // 52
    ( $lf2l    0      $Ws )  // 53
    ( $lf2l    $Hs    0   )  // 54
    ( $lf2l    $Hs    $Ws )  // 55
    ( $lf2l   -$Hs    0   )  // 56
    ( $lf2l   -$Hs    $Ws )  // 57
    ( $lf2l   -$ymin  0   )  // 58
    ( $lf2l   -$ymin  $Ws )  // 59
    ( $lf2l_t  $ycd   0   )  // 60
    ( $lf2l_t  $ycd   $Ws )  // 61

    // --- x = rf2l ---
    ( $rf2l    0      0   )  // 62
    ( $rf2l    0      $Ws )  // 63
    ( $rf2l    $Hs    0   )  // 64
    ( $rf2l    $Hs    $Ws )  // 65
    ( $rf2l   -$Hs    0   )  // 66
    ( $rf2l   -$Hs    $Ws )  // 67
    ( $rf2l   -$ymin  0   )  // 68
    ( $rf2l   -$ymin  $Ws )  // 69
    ( $rf2l_t  $ycd   0   )  // 70
    ( $rf2l_t  $ycd   $Ws )  // 71

    // --- x = lf1l ---
    ( $lf1l    0      0   )  // 72
    ( $lf1l    0      $Ws )  // 73
    ( $lf1l    $Hs    0   )  // 74
    ( $lf1l    $Hs    $Ws )  // 75
    ( $lf1l   -$Hs    0   )  // 76
    ( $lf1l   -$Hs    $Ws )  // 77
    ( $lf1l   -$ymin  0   )  // 78
    ( $lf1l   -$ymin  $Ws )  // 79
    ( $lf1l_t  $ycd   0   )  // 80
    ( $lf1l_t  $ycd   $Ws )  // 81

    // --- x = rf1l ---
    ( $rf1l    0      0   )  // 82
    ( $rf1l    0      $Ws )  // 83
    ( $rf1l    $Hs    0   )  // 84
    ( $rf1l    $Hs    $Ws )  // 85
    ( $rf1l   -$Hs    0   )  // 86
    ( $rf1l   -$Hs    $Ws )  // 87
    ( $rf1l   -$ymin  0   )  // 88
    ( $rf1l   -$ymin  $Ws )  // 89
    ( $rf1l_t  $ycd   0   )  // 90
    ( $rf1l_t  $ycd   $Ws )  // 91

    // --- x = lf0 ---
    ( $lf0     0      0   )  // 92
    ( $lf0     0      $Ws )  // 93
    ( $lf0     $Hs    0   )  // 94
    ( $lf0     $Hs    $Ws )  // 95
    ( $lf0    -$Hs    0   )  // 96
    ( $lf0    -$Hs    $Ws )  // 97
    ( $lf0    -$ymin  0   )  // 98
    ( $lf0    -$ymin  $Ws )  // 99
    ( $lf0_t   $ycd   0   )  // 100
